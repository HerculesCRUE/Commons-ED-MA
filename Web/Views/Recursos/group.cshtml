            url = localUrlBase + "Hercules/DatosGraficaPublicaciones"
        }
        var arg = {};
        arg.pParametros = parametros;
        $('#' + idContenedor).empty();
        $.get(url, arg, function (data) {
            PintarGraficaPublicaciones(data, idContenedor);
        });
    }

