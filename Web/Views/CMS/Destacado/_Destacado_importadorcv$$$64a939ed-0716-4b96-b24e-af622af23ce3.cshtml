<div id="containerCV">
	<div class="row">
		<div class="col col-12 col-breadcrumb">
			<ul>
				<li>
					<a href="@Comunidad.Url">Home</a>
				</li>
				<li>@Html.Translate("IMPORTAR_CV")</li>
			</ul>
		</div>
		<div class="col col-12 col-xl-8 col-contenido izquierda paso1">
			<div class="wrapCol">
				<div class="header-contenido">
					<div class="h1-container">
						<h1>@Html.Translate("IMPORTA_TU_CVN")</h1>
					</div>
				</div>
				<p>@Html.Translate("IMPORTA_TU_CVN_CON_UN_CLICK")</p>
				<input type="file" name="" id="file_cvn" accept="application/pdf" data-title-text="@Html.Translate("SUBE_PDF_CV")" data-size-text="@Html.Translate("MAXIMO_PESO_CV")" data-format-text="@Html.Translate("FORMATOS_ADMITIDOS_CV")" data-format-error="@Html.Translate("FORMATO_ERROR_CV")" data-size-error="@Html.Translate("TAMANIO_ERROR_CV")">
				<br>
				<div class="botonera">
					<a href="javascript: void(0);" class="btn btn-primary uppercase btProcesarCV">@Html.Translate("PROCESAR")</a>
				</div>
			</div>
		</div>
		<div class="col col-12 col-contenido paso2" style="display:none">
			<div class="wrapCol">
				<div class="cabecera-cv">
					<div class="upper-wrap">
						<div class="h1-container">
							<h1>@Html.Translate("IMPORTA_TU_CVN")</h1>
							<div id="dropdown_seleccion_cv" class="seleccionar dropdown dropdown-select">
								<a class="dropdown-toggle" data-toggle="dropdown" aria-expanded="true">
									<span class="material-icons">done_all</span>
								</a>
								<div class="dropdown-menu basic-dropdown dropdown-menu-right" x-placement="bottom-end">
									<a id="CV_select_all" class="item-dropdown"><span class="material-icons">done_all</span><span class="texto">Seleccionar todos</span></a>
									<a id="CV_unselect_all" class="item-dropdown"><span class="material-icons">remove_done</span><span class="texto">Deseleccionar todos</span></a>
								</div>
							</div>					
							<div id="dropdown_mostrar_cv" class="seleccionar dropdown dropdown-select">
								<a class="dropdown-toggle" data-toggle="dropdown" aria-expanded="true">
									<span class="material-icons">preview</span>
								</a>
								<div class="dropdown-menu basic-dropdown dropdown-menu-right" x-placement="bottom-end">
									<a class="item-dropdown mostrarTodos"><span class="texto">Mostrar todos</span></a>
									<a class="item-dropdown mostrarSimilitudes"><span class="texto">Mostrar similitudes</span></a>
									<a class="item-dropdown mostrarNuevos"><span class="texto">Mostrar nuevos</span></a>
								</div>
							</div>							
						</div>						
					</div>
				</div>
				<div class="alert" id="informacionImportacion">
					<p class="alert-title">
						<span class="material-icons">warning</span>
						@Html.Translate("INFORMACION_IMPORTACION")
						@Html.Raw(Html.Translate("DETALLES_INFORMACION_IMPORTACION"))
					</p>
				</div>
				<div class="contenido-cv">
				</div>
				<a class="btn btn-primary uppercase btImportarCV" >@Html.Translate("IMPORTAR")</a>
			</div>
		</div>
	</div>
</div>
