<div id="containerCV">
	<div class="row">
		<div class="col col-12 col-breadcrumb">
			<ul>
				<li>
					<a href="@Comunidad.Url">Home</a>
				</li>
				<li>@Html.Translate("EXPORTAR_CV")</li>
			</ul>
		</div>
		<div class="col col-12 col-contenido listadoExportacion">
			<div class="wrapCol">
				<div class="header-contenido">
					<div class="h1-container">
						<h1>@Html.Translate("EXPORTA_TU_CVN")</h1>
					</div>
				</div>
				<p>@Html.Translate("AYUDAMOS_GESTIONAR_RESULTADOS_INVESTIGACIÃ“N")</p>
				<div class="page-section">
					<div class="page-section-title">
						<span class="material-icons">backup</span>
						<h2>@Html.Translate("DESCARGAR_CVN")</h2>
					</div>
					<div class="form-group mb-3">
						<label class="control-label d-block">@Html.Translate("TITULO")</label>
						<input id="exportCvName" placeholder="@Html.Translate("INTRODUCE_UN_TITULO_PARA_EL_FICHERO")" type="text" class="form-control not-outline">
					</div>
					<div class="form-group mb-3 ddlIdioma">
						<label class="control-label d-block">@Html.Translate("IDIOMA")</label>
						<select id="ddlIdioma" name="ddlIdioma" class="js-select2">
							<option value="es" selected>@Html.Translate("ESPAGNOL")</option>
							<option value="en">@Html.Translate("INGLES")</option>
							<option value="ca">@Html.Translate("CATALAN")</option>
							<option value="eu">@Html.Translate("EUSKERA")</option>
							<option value="gl">@Html.Translate("GALLEGO")</option>
							<option value="fr">@Html.Translate("FRANCES")</option>
						</select>
					</div>
					<div class="form-group mb-3">
						<label class="control-label d-block ddlTipoExportacion">@Html.Translate("TIPO_EXPORTACION")</label>
						<select id="ddlTipoExportacion" name="ddlTipoExportacion" class="js-select2">
							<option value="PN2008" selected>CVN</option>
							<option value="CVAISCIII">CVA-ISCIII</option>
							<option value="CVA2015">CVA-AEI</option>
						</select>
					</div>
					<div class="form-group mb-3">
						<label class="control-label d-block ddlVersionExportacion">@Html.Translate("VERSION_EXPORTACION")</label>
						<select id="ddlVersionExportacion" name="ddlVersionExportacion" class="js-select2">
							<option value="1_4_0">1.4.0</option>
							<option value="1_4_3" selected>1.4.3</option>
						</select>
					</div>
					<div class="form-group mb-3 ddlSecciones">
						<label class="control-label d-block mb-3">@Html.Translate("SECCIONES")</label>
						<div class="custom-control themed little custom-radio">
							<input type="radio" checked="checked" id="todas" name="secciones" class="custom-control-input">
							<label class="custom-control-label" for="todas">@Html.Translate("TODAS")</label>
						</div>
						<div class="custom-control themed little custom-radio">
							<input type="radio" id="seleccionar" name="secciones" class="custom-control-input">
							<label class="custom-control-label" for="seleccionar">@Html.Translate("SELECCIONAR")</label>
						</div>
						<div class="custom-control themed little custom-radio">
							<input type="radio" id="ultimosCinco" name="secciones" class="custom-control-input">
							<label class="custom-control-label" for="ultimosCinco">@Html.Translate("ULTIMOS_CINCO_ANIOS")</label>
						</div>
						<a class="btn btn-primary uppercase btGenerarExportarCV" >@Html.Translate("GENERAR_NUEVA_EXPORTACION")</a>
					</div>
				</div>
				<div class="page-section">
					<div class="page-section-title">
						<span class="material-icons">settings_input_component</span>
						<h2>@Html.Translate("CVN_GENERADOS")</h2>
					</div>
					<div class="resource-list listView">
						<div class="resource-list-wrap listadoCV">
						</div>
						<div class="panNavegador">
						</div>
					</div>
				</div>
			</div>
		</div>
		<div class="col col-12 col-contenido exportacion" style="display:none">
			<div class="wrapCol">
				<div class="cabecera-cv">
					<div class="upper-wrap">
						<div class="h1-container">
							<h1>@Html.Translate("EXPORTA_TU_CVN")</h1>
						</div>
					</div>
				</div>				
				<div class="selector-cv">
					<div class="page-section">
						<div class="page-section-title">
							<span class="material-icons">fact_check</span>
							<h2>@Html.Translate("PERFIL_DE_EXPORTACION")</h2>
						</div>
						<div class="form-group mb-3">
							<label class="control-label d-block">@Html.Translate("NOMBRE_DEL_PERFIL")</label>
							<input placeholder="@Html.Translate("ASIGNE_UN_NOMBRE")" type="text" class="form-control not-outline nombrePerfilExportacion">
							<br>
							<a class="btn btn-primary uppercase btGuardarCV">@Html.Translate("GUARDAR")</a>
						</div>
						<div class="form-group mb-3 misPerfiles">
							<label class="control-label d-block">@Html.Translate("MIS_PERFILES"):</label>
							<select id="ddlProfile" name="ddlProfile" class="js-select2" data-select-search="true">								
							</select>
							<a class="btn btn-primary uppercase btCargarCV">@Html.Translate("CARGAR")</a>
							<a class="btn btn-primary uppercase btBorrarCV">@Html.Translate("BORRAR")</a>
						</div>
					</div>
				</div>				
				<div class="contenido-cv">
				</div>
				<a class="btn btn-primary uppercase btExportarCV" >@Html.Translate("EXPORTAR")</a>
			</div>
		</div>
	</div>	
</div>
<script>
$(document).ready(function () {
	exportacionCV.init();
});
</script>